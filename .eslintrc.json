{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["google", "eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn",
    "no-unused-vars": "error",
    "no-undef": "error",
    "no-use-before-define": "error",
    "no-undef-init": "error",
    "camelcase": "error",
    "eqeqeq": "error",
    "new-cap": "off"
  }
}
